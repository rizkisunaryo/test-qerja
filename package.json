    "redux": "4.0.1",
    "redux-logger": "3.0.6",
    "redux-persist": "^5.10.0",
    "redux-saga": "0.16.2",
    "redux-thunk": "2.3.0",
    "reduxsauce": "1.0.1",
    "seamless-immutable": "7.1.4"
  },
  "devDependencies": {
